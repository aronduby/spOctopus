{
  "manifest_version": 2,
  "name": "spOctopus",
  "version": "0.1",
  "web_accessible_resources": [
    "dibs/loader.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://deploy.salespad.net/*"
      ],
      "css": [
        "matrix-scrolling/matrix-scrolling.css"
      ],
      "js": [
        "matrix-scrolling/jquery-3.0.0.min.js",
        "matrix-scrolling/matrix-scrolling.js"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://deploy.salespad.net/*"
      ],
      "js": [
        "dibs/dist/vendor.min.js",
        "dibs/dist/app.js"
      ],
      "css": [
        "dibs/dist/app.css"
      ]
    }
  ]
}